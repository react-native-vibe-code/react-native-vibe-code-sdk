{
  "name": "capsule-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "ln -sf ../../.env.local apps/web/.env.local && echo 'Symlinked .env.local to apps/web/'",
    "dev": "turbo run dev --concurrency=23",
    "dev:light": "turbo run dev:app --filter=@react-native-vibe-code/web",
    "dev:watch": "turbo watch build --filter='./packages/*'",
    "dev:web": "turbo run dev --filter=@react-native-vibe-code/web",
    "dev:docs": "turbo run dev --filter=@react-native-vibe-code/docs",
    "build": "turbo run build",
    "build:force": "turbo run build --force",
    "build:web": "turbo run build --filter=@react-native-vibe-code/web",
    "build:web:force": "turbo run build --filter=@react-native-vibe-code/web --force",
    "build:packages": "turbo run build --filter='./packages/*'",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build:packages && changeset publish",
    "db:generate": "turbo run db:generate --filter=@react-native-vibe-code/database",
    "db:migrate": "turbo run db:migrate --filter=@react-native-vibe-code/database",
    "db:studio": "turbo run db:studio --filter=@react-native-vibe-code/database",
    "db:push": "turbo run db:push --filter=@react-native-vibe-code/database",
    "build:agent": "turbo run build:standalone --filter=@react-native-vibe-code/agent",
    "build:sandbox:expo": "turbo run build:sandbox:expo --filter=@react-native-vibe-code/sandbox",
    "build:sandbox:tamagui": "turbo run build:sandbox:tamagui --filter=@react-native-vibe-code/sandbox",
    "build:sandbox:github": "turbo run build:sandbox:github --filter=@react-native-vibe-code/sandbox",
    "build:sandbox:expo-testing": "turbo run build:sandbox:expo-testing --filter=@react-native-vibe-code/sandbox",
    "x-bot:register-webhook": "npx tsx apps/web/scripts/register-x-webhook.ts",
    "e2b:kill-all": "e2b sandbox kill --all",
    "e2b:kill": "e2b sandbox kill",
    "e2b:connect": "e2b sandbox connect",
    "e2b:logs": "e2b sandbox logs",
    "docker:prune": "docker system prune -af --volumes",
    "inngest:dev": "npx inngest-cli@latest dev",
    "clean": "yarn clean:turbo & yarn clean:node-modules & yarn clean:build",
    "clean:turbo": "rm -rf ./node_modules/.cache/turbo",
    "clean:build": "rm -rf apps/.next",
    "clean:node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
